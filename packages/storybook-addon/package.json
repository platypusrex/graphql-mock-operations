{
  "name": "@graphql-mock-operations/storybook-addon",
  "version": "0.0.1",
  "author": "Frank Cooke",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "files": [
    "dist/**/*",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build:cjs": "swc src -d dist/cjs --config-file ../../.swcrc -C module.type=commonjs",
    "build:esm": "swc src -d dist/esm --config-file ../../.swcrc -C module.type=es6",
    "build:ts": "tsc",
    "build": "yarn clean && concurrently \"yarn build:cjs\" \"yarn build:esm\" \"yarn build:ts\"",
    "clean": "rimraf dist",
    "clean:deps": "rimraf node_modules",
    "check:types": "tsc --project tsconfig.type.json"
  },
  "devDependencies": {
    "@apollo/client": "^3.5.9",
    "@storybook/react": "^6.4.19",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.177",
    "@types/react": "^17.0.39",
    "concurrently": "^7.0.0",
    "graphql": "^16.3.0",
    "react": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@storybook/addons": "^6.4.19",
    "@storybook/components": "^6.4.19"
  },
  "peerDependencies": {
    "@apollo/client": "^3.*",
    "@storybook/react": "^6.4.19",
    "react": "^17.*"
  }
}
